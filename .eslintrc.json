{
    "extends": "airbnb-base",
    "parser": "babel-eslint",
    "env": {
        "es6": true,
        "browser": true,
        "jest": true
    },
    "rules": {
        "no-restricted-syntax": [
            "error",
            "LabeledStatement",
            "WithStatement"
        ],
        "import/no-named-as-default": 0,
        "import/no-named-as-default-member": 0
    }
}